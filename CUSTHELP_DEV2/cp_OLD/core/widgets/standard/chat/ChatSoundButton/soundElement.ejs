<rn:block id="preAudio"/>
<% if(oggEnabled || wavEnabled) { %>
    <audio id="rn_<%= instanceID %>_ReceiveSound">
    <% if(oggEnabled){ %>
        <source src="<%= attrs.receive_sound_ogg_path %>"/>
    <% } else if(wavEnabled){ %>
        <source src="<%= attrs.receive_sound_wav_path %>"/>
    </audio>
    <% }
} else if(attrs.enable_flash_support && attrs.receive_sound_flash_path) {%>
    <div id="rn_<%= instanceID %>_ReceiveSoundFlashContainer" class="rn_FlashContainer"/>
<% } %>
<rn:block id="postAudio"/>
